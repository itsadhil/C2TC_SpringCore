<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="student1" class="com.tnsif.springcore_example.student">
		<property name="name" value="Om">
		</property>
	</bean>
	
	<bean id="student2" class="com.tnsif.springcore_example.student">
		<property name="name" value="Sabari">
		</property>
	</bean>
	
	<bean id="mvit" class="com.tnsif.springcore_example.college">
		<property name="s1" ref="student1"></property>
	</bean>
	
	<bean id="smvec" class="com.tnsif.springcore_example.college">
		<property name="s1" ref="student2"></property>
	</bean>
</beans>